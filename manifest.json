{
  "manifest_version": 2,
  "name": "Something Extension",
  "version": "1.6",
  "description": "色々やる個人的なExtenstion。",

  "content_scripts": [
    {
      "matches": ["https://www.nicovideo.jp/watch/*"],
      "js": ["nicovideo.js"]
    }
  ],

  "permissions": [
    "tabs",
    "storage",
    "contextMenus"
  ],

  "browser_action": {
    "default_title": "popup",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "image/icon-16.png",
      "24": "image/icon-24.png",
      "32": "image/icon-32.png"
    }
  },

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "commands": {
  }

}
