{
  "manifest_version": 2,
  "name": "Something Extension",
  "version": "0.1",
  "description": "色々やる個人Extenstion. - ニコ動での動画上広告を削除する等",

  "content_scripts": [
    {
      "matches": ["https://www.nicovideo.jp/watch/*"],
      "css": [],
      "js": ["javascript/nicovideo.js"]
    }
  ],

  "permissions": [
    "tabs",
    "activeTab"
  ],

  "browser_action": {
    "default_title": "popup",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "image/icon-16.png",
      "24": "image/icon-24.png",
      "32": "image/icon-32.png"
    }
  },

  "options_page": "options.html"

}
